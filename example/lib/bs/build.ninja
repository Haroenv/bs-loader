ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name example
src_root_dir = /Users/ryan/Documents/dev/get-typed-webpack-plugin/example
bsc = /Users/ryan/Documents/dev/get-typed-webpack-plugin/example/node_modules/bs-platform/bin/bsc.exe
bsdep = /Users/ryan/Documents/dev/get-typed-webpack-plugin/example/node_modules/bs-platform/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I /Users/ryan/Documents/dev/get-typed-webpack-plugin/example/node_modules/bs-platform/lib/ocaml -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
refmt = /Users/ryan/Documents/dev/get-typed-webpack-plugin/example/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = -ppx '/Users/ryan/Documents/dev/get-typed-webpack-plugin/example/node_modules/bs-platform/bin/reactjs_jsx_ppx.exe'
refmt_flags = --print binary
bsb_dir_group = 0
bsc_lib_includes = -I types -I src
rule build_ast_and_deps_from_reason_impl
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/print_math.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/print_math.re
rule build_deps
  command = ${bsdep} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/print_math.mlast.d : build_deps src/print_math.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/print_math.cmj $src_root_dir/lib/es6/src/print_math.js src/print_math.cmi : build_cmj_cmi src/print_math.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output es6:lib/es6/src
build types/math.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/types/math.re
build types/math.mlast.d : build_deps types/math.mlast
build types/math.cmj $src_root_dir/lib/es6/types/math.js types/math.cmi : build_cmj_cmi types/math.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output es6:lib/es6/types
build types/index.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/types/index.re
build types/index.mlast.d : build_deps types/index.mlast
build types/index.cmj $src_root_dir/lib/es6/types/index.js types/index.cmi : build_cmj_cmi types/index.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output es6:lib/es6/types
build build.ninja : phony  ||  types/index.mlast.d types/math.mlast.d src/print_math.mlast.d
